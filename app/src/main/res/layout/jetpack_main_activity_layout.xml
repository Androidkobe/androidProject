<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/mJectpack_WorkManager"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:text="workManager"
        android:textAllCaps="false"
        app:layout_constraintDimensionRatio="16:9" />

    <Button
        android:id="@+id/mJectpack_LiveData"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintLeft_toRightOf="@+id/mJectpack_WorkManager"
        android:text="LiveData"
        android:textAllCaps="false"
        app:layout_constraintDimensionRatio="16:9" />

    <Button
        android:id="@+id/mJectpack_DataStore"
        android:layout_width="0dp"
        android:layout_height="50dp"
        app:layout_constraintLeft_toRightOf="@+id/mJectpack_LiveData"
        android:text="DataStore"
        android:textAllCaps="false"
        app:layout_constraintDimensionRatio="16:9" />


    <Button
        android:id="@+id/mJectpack_Data_binding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="dataBinding"
        android:textAllCaps="false"
        app:layout_constraintTop_toBottomOf="@+id/mJectpack_WorkManager" />

    <Button
        android:id="@+id/mJectpack_Navigation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAllCaps="false"
        android:text="Navigation"
        app:layout_constraintLeft_toRightOf="@+id/mJectpack_Data_binding"
        app:layout_constraintTop_toBottomOf="@+id/mJectpack_WorkManager"
        app:layout_constraintDimensionRatio="16:9" />
</androidx.constraintlayout.widget.ConstraintLayout>