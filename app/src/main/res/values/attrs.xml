<resources>
    <declare-styleable name="FullscreenAttrs">
        <attr name="fullscreenBackgroundColor" format="color" />
        <attr name="fullscreenTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="FlikerProgressBar">
        <attr name="textSize" format="dimension"/>
        <attr name="loadingColor" format="color"/>
        <attr name="stopColor" format="color"/>
        <attr name="radius" format="dimension"/>
        <attr name="borderWidth" format="dimension"/>
    </declare-styleable>


    <declare-styleable name="DragBubbleView">
        <attr name="dvtextSize" format="dimension" />
        <attr name="bubbleRadius" format="dimension" />
        <attr name="bubbleColor" format="color" />
        <attr name="dvtext" format="string" />
        <attr name="dvtextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="SliderLockView">
        <attr name="sliderSpace" format="dimension" />
        <attr name="textsize" format="dimension" />
        <attr name="textcolor" format="color" />
        <attr name="text" format="string" />
    </declare-styleable>


    <declare-styleable name="CustomTimeView">
        <attr name="time" format="string" />
    </declare-styleable>

    <attr name="header_view" format="reference" />
    <attr name="target_view" format="reference" />
    <attr name="header_init_offset" format="dimension" />
    <attr name="target_init_offset" format="dimension" />
    <declare-styleable name="EventDispatchPlanLayout">
        <attr name="header_view" />
        <attr name="target_view" />
        <attr name="header_init_offset" />
        <attr name="target_init_offset" />
    </declare-styleable>

    <declare-styleable name="NestingScrollPlanLayout">
        <attr name="header_view" />
        <attr name="target_view" />
        <attr name="header_init_offset" />
        <attr name="target_init_offset" />
    </declare-styleable>

    <!--涟漪控件属性-->
    <declare-styleable name="mRippleView">
        <attr name="cColor" format="color" />
        <attr name="cSpeed" format="integer" />
        <attr name="cDensity" format="integer" />
        <attr name="cIsFill" format="boolean" />
        <attr name="cIsAlpha" format="boolean" />
    </declare-styleable>
</resources>