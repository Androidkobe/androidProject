        <activity android:name=".sundu.developer.stickyheadrecyclerview.StickyHeadRecyclerViewActivity" />
        <activity android:name=".sundu.developer.recycleview.RecyclerViewActivity" />
        <activity android:name=".sundu.developer.sensor.SenSorActivity" />
        <activity android:name=".sundu.jetpack.databinding.bidirectional.BidirectionalActivity" />
