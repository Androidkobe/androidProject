        <activity android:name=".sundu.developer.immersive.ImmerSiveActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sundu.kotlin.KotlinActivity" />
        <activity
